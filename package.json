{
  "name": "reactjsbasics",
  "version": "1.0.0",
  "description": "Playing with ReactJs",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
